public class Tester {

	public static void main(String[] args) throws IOException {
		String s="C:/Users/Guille/Desktop/Uni/Tecnología de Programación/Proyecto/Repositorio/Mapas/Mapa 1.txt";
/*		Mapa mapa= new Mapa(s);
		Iterator<Character> it;
