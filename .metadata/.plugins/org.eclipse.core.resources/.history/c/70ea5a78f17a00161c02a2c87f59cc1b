package Obstaculo;

import Mapa.*;
import javax.swing.*;

public class Obstaculo extends Celda{
	private ImageIcon imagen;
	
	public Obstaculo(String nom, int X, int Y){
		this.setNombre(nom);
		imagen= new ImageIcon("/Imagenes/"+this.getNombre()+".png");
		this.setX(X);
		this.setY(Y);
	}
	public ImageIcon getImagen(){
		return imagen;
	}
}