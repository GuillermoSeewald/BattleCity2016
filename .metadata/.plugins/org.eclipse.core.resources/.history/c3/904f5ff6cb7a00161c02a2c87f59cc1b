package Obstaculo;

import Mapa.*;
import javax.swing.*;

public class Obstaculo extends Celda{
	private JLabel imagen;
	private int resistencia;
	
	public Obstaculo(String nom){
		this.setNombre(nom);
		imagen = new JLabel();
		imagen.setIcon(new ImageIcon("/Imagenes/"+this.getNombre()+".png"));
	}
	
	public void setResistencia(int res){
		resistencia=res;
	}
	public int getResistencia(){
		return resistencia;
	}
}