		while((cadena=b.readLine())!=null){
			c=cadena.toCharArray();
			for(int i=0;i<c.length;i++){
				if(c[i]==('A')){
					
				}
			}
		}
	}
	
	
	
	public int getLongitud(){
		return obstaculos.length;
	}
}