package Obstaculo;

import Mapa.*;
import javax.swing.*;

public class Obstaculo extends Celda{
	private JLabel imagen;
	
	public Obstaculo(String nom, int X, int Y){
		this.setNombre(nom);
		imagen = new JLabel();
		imagen.setIcon(new ImageIcon("/Imagenes/"+this.getNombre()+".png"));
		this.setX(X);
		this.setY(Y);
	}
}