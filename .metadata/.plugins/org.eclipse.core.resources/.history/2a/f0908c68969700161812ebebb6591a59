package Logic.Map;

import Logic.Tank.*;
import Graphic.Map.*;
import java.awt.Image;
import javax.swing.ImageIcon;

public abstract class Element{
	
	protected String nombre;
	protected Map map;
	protected int posX;
	protected int posY;
	protected GraphicElement graphic;
	
	public Element(String nom, int X, int Y, Map map, int posx, int posy){
		graphic= new GraphicElement(X,Y,nom);
		this.map=map;
		posX=posx;
		posY=posy;
		nombre=nom;
	}
	//X de matriz
	public int getPosX(){
		return posX;
	}
	//Y de matriz
	public int getPosY(){
		return posY;
	}
	protected abstract int kill();
	public abstract int kill(Player pla);
	public abstract int kill(Enemy ene);
/*	public abstract boolean collide(Player jug);
	public abstract boolean collide(Enemy ene);*/
}